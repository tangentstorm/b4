{script:home.0}
on view do
  other.value: field.value
  wait[10]
  go[card] 
end

on update_toc do
  known: extract name from t:grid1.value
  t:update exists:name in keys deck.cards from t
  if new: extract value where !value in known from keys deck.cards
    t:t,select ready:0 exists:2 name:value note:"" from new
  end
  grid1.value:t
end


on goto_selected do
  go[grid1.value.name[grid1.row] "SlideUp"]
end
{end}

{script:home.1}
on click do
  update_toc[]
end

{end}

{script:home.2}
on click do
 goto_selected[]
end
{end}

