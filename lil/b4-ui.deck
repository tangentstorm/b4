{contraption:terminal}
size:[512,336]
margin:[4,4,4,4]
script:"terminal.0"
attributes:{"name":["tw","th","cx","cy","fg","bg"],"label":["term width","term height","cursor x","cursor y","fg color (ansi)","bg color (ansi)"],"type":["number","number","number","number","number","number"]}
{widgets}
font:{"type":"field","size":[264,112],"pos":[528,0],"show":"transparent","value":"%%"}
chart:{"type":"canvas","size":[128,256],"pos":[-160,0],"locked":1,"script":"vgafont.2","image":"%%","pattern":0,"scale":1}
term:{"type":"canvas","size":[513,336],"pos":[0,0],"script":"hexcalc.1","border":0,"pattern":47,"scale":1}
fg:{"type":"slider","size":[48,16],"pos":[528,112],"show":"transparent","interval":[0,15],"value":7,"style":"compact"}
bg:{"type":"slider","size":[48,16],"pos":[576,112],"show":"transparent","interval":[0,15],"style":"compact"}
cx:{"type":"slider","size":[48,16],"pos":[528,128],"show":"transparent","interval":[0,63],"style":"compact"}
cy:{"type":"slider","size":[48,16],"pos":[576,128],"show":"transparent","interval":[0,20],"style":"compact"}
tw:{"type":"slider","size":[48,16],"pos":[528,144],"show":"transparent","interval":[0,100],"value":64,"style":"compact"}
th:{"type":"slider","size":[48,16],"pos":[576,144],"show":"transparent","interval":[0,25],"value":20,"style":"compact"}

{script:terminal.0}
cs: 8,16  # character size
mb4: deck.modules.mb4.value

# terminal width/height attributes
on get_tw do tw.value end   on set_tw x do tw.value:x cp.max:tw.value-1 end
on get_th do th.value end   on set_th x do th.value:x cy.max:th.value-1 end

# cursor
on get_cx do cx.value end   on set_cx x do cx.value:x end
on get_cy do cy.value end   on set_cy x do cy.value:x end

# attributes
# ansi color -> b4 palette
# ( extract i orderby vga asc from b4.value ) from palette card
to_b4: 32 + 15,3,8,5,11,14,9,2,1,4,7,6,12,13,10,0
on to_ansi x do first extract index where value=x from to_b4 end
on get_fg do fg.value end   on set_fg x do fg.value:x end
on get_bg do bg.value end   on set_bg x do bg.value:x end


on cxy ci do gw:16  cs*(gw%ci),floor ci/gw  end
on blit ci fg bg xy do  # ci= character index
  im:chart.copy[cxy[ci] cs].map[(0,1) dict to_b4@bg,fg]
  term.paste[im xy * cs]
end

on clr_scr do
  term.pattern: to_b4[bg.value]
  term.rect[0,0 term.size]
  cx.value: cy.value:0
end

on clr_eol do
 term.pattern: to_b4[bg.value]
 each i in cx.value + range tw.value-cx.value
   blit[32 fg.value bg.value i,cy.value]
 end
end

op.g: on goxy do cy.value:mb4.dpop[] cx.value:mb4.dpop[] end
op.a: on attr do a:mb4.dpop[]
  fg.value: bits.and[a,15]
  bg.value: floor a/16
end
op.e: on emit do
  ci: bits.and[mb4.dpop[], 255]
  x:cx.value y:cy.value
  blit[ci fg.value bg.value x,y]
  # todo: scrolling (for now it's just a torus)
  cx.value: if x<tw.value-1 x+1 else y:y+1 0 end
  cy.value: if y<th.value-1 y else 0 end
end
op.r: on rkey vm do mb4.dput[0] end # no readkey
op.k: on keyp vm do mb4.dput[0] end
op.s: on cscr vm do clr_scr[] end
op.l: on ceol vm do clr_eol[] end
op.c: on quxy vm do mb4.dput[cx.value] mb4.dput[cy.value] end
on get_op do op end
{end}

