size:[435,200]
margin:[0,0,0,0]
script:"b4_debugger.0"
{widgets}
dbg_top:{"type":"slider","size":[15,200],"pos":[319,0],"script":"b4_debugger.1","interval":[0,4095],"step":64,"format":"%.2H","style":"vert"}
mem_view:{"type":"canvas","size":[320,200],"pos":[0,0],"script":"hexcalc.1","font":"mono","image":"%%","brush":90,"pattern":0,"scale":1}
