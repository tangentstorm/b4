{deck}
version:1
card:3
size:[866,510]
script:"0"
name:"b4-ui.deck"
patterns:"%%"

{script:0}
on navigate dir do
  c:deck.card
  if c.index=0 # home/map card
    if dir="down" go["Back" "SlideUp"] end
  elseif dir="left"
    d:if c.index=1 "Last" else "Prev" end
    go[d "SlideRight"]
  elseif dir="right"
    d:if c.index=(count deck.cards)-1 (keys deck.cards)[1] else "Next" end
    go[d "SlideLeft"]
  elseif dir="up" go["First" "SlideDown"] end
end

on run_b4i text do
  if !con:deck.card.widgets.b4con panic["no 'b4con' widget found"] end
  con.clear[]
  each line in "\n" split text
    if !line[0]="#" con.run[line] end
  end
  deck.card.event["b4_step"]
end

on run_lil text do
  d.mb4: deck.modules.mb4.value
  eval[text d 1]
end

{end}

