description:"A tab strip made of buttons.\n\nemits event : set_tab[widget_name tab_name proto]"
script:"tabstrip.0"
attributes:{"name":["tnames","wnames","afg","abg","ifg","ibg"],"label":["tab names","widget names","active fg color","active bg color","inactive fg color","inactive bg color"],"type":["string","string","number","number","number","number"]}
{widgets}
inactive_bg:{"type":"slider","size":[48,16],"pos":[64,-128],"interval":[0,47],"value":13,"style":"bar"}
wnames:{"type":"field","size":[384,16],"pos":[0,-40],"value":"w1|w2|w3"}
twidths:{"type":"field","size":[384,16],"pos":[0,-71],"style":"plain","value":"22|22|22"}
which:{"type":"slider","size":[48,16],"pos":[336,-128],"script":"tabstrip.1","interval":[0,3],"style":"compact"}
canvas1:{"type":"canvas","size":[438,20],"pos":[0,0],"locked":1,"volatile":1,"font":"menu","show":"transparent","border":0,"scale":1}
tnames:{"type":"field","size":[384,16],"pos":[0,-100],"value":"t1|t2|t3"}
active_fg:{"type":"slider","size":[48,16],"pos":[0,-160],"interval":[0,47],"value":1,"style":"bar"}
inactive_fg:{"type":"slider","size":[48,16],"pos":[0,-128],"interval":[0,47],"value":1,"style":"bar"}
active_bg:{"type":"slider","size":[48,16],"pos":[64,-160],"interval":[0,47],"value":32,"style":"bar"}

{script:tabstrip.0}
on which_font do
  # card.font doesn't exist on the prototype
  if card.font card.font else deck.fonts.menu end
end

on change do view[] end
on view do draw[] end

on draw do
  # -- redraw the tabs
  tx:0 nt:count tns: "|" split tnames.text
  cv:canvas1  cv.font:f:which_font[]
  ch:cv.size[1]  p:0
  cv.pattern:0  cv.rect[0,0 cv.size]  cv.brush:0
  tws: 10 + nws:first@f.textsize@tns # tab and name widths
  twidths.text: "|" fuse tws   which.interval: 0,count tws  # which=count->select none
  each tn i in tns
    cv.pattern: if i=which.value active_bg.value else inactive_bg.value end
    cv.rect[xy:p,0 wh:tws[i],ch]
    cv.pattern: 1  cv.box[xy wh]
    cv.pattern: if i=which.value active_fg.value else inactive_fg.value end
    cv.text[tn xy+5]
    p: p + tws[i]-1
  end
  if -1<ix:which.value
    wns:"|"split wnames.text   wn:wns[ix]
    tns:"|"split tnames.text   tn:tns[ix]
    dc:deck.card
    (dc.widgets@wns)..show:"none"
    dc.widgets[wn].show:"solid"
    deck.card.event["set_tab" wn tn card]
  end  
end

on click do
  p:0 mx:(pointer.pos-me.offset)[0] ix:-1
  each tw i in 0+"|"split twidths.text
    if (ix=-1) & mx<p:p+tw
      which.value: ix: i
    end
  end
  draw[]
  if ix>-1
    wns:"|"split wnames.text   wn:wns[ix]
    tns:"|"split tnames.text   tn:tns[ix]
    deck.card.event["set_tab" wn tn card]
  end
end


on get_abg   do active_bg.value   end
on set_abg x do active_bg.value:x end
on get_afg   do active_fg.value   end
on set_afg x do active_fg.value:x end
on get_ibg   do inactive_bg.value   end
on set_ibg x do inactive_bg.value:x end
on get_ifg   do inactive_fg.value   end
on set_ifg x do inactive_fg.value:x end

on get_tnames do tnames.text end
on get_wnames do wnames.text end
on set_wnames x do wnames.text:x end
on set_tnames x do tnames.text:x end
on get_active x do which.value end
on set_active x do which.value:x view[] end

{end}

{script:tabstrip.1}
on change val do
  view[]
end
{end}

