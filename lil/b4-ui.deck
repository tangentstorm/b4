{script:cv-demo.0}
mb4.set_echo[on echo line do
  t:output.text
  output.text: if t t,"\n",line else line end
  output.scroll: 999999
end]
mb4.set_cv[canvas1]
on view do
  status.text
    : mb4.showStack["ds"], "\n"
    , mb4.showStack["cs"], "\n"
    , mb4.showIp[]
end


on run line do
 echo["> ",line]
 mb4.b4i[line]
 view[]
end

on click do
  run[me.text]
  show_ram[]
end

on run_b4i text do 
  output.text:""
  each line in "\n" split text
    if !line[0]="#" run[line] end
  end
  show_ram[]
end

on run_lil text do
  eval[text 0 1]
end

on show_ram do
 # start on debugger. draw some ram, highlight ip
 ip:mb4.get_vm[].ip
 a0: 256 gw:16 mw:31 cs: 18,18  ofs:3,4  
 on gxy i do (gw%i),floor i/gw end
 ixy: (ofs+(mw,0)-(3,3))+cs*gxy[ip-a0]+0,1
 c: card.widgets.debugger
 c.pattern: 47 c.rect[0,0 c.size]        # bg
 c.pattern: 35  c.rect[ixy cs+0,1]       # ip
 c.pattern: 41 c.rect[0,0 mw,c.size[1]]  # hex
 c.rect[0,0 c.size[0],cs[1]+ofs]
 c.pattern: 0 c.font: "mono"
 c.text[(list " +%H") format range 16 ofs+25,2]
 c.pattern: 0 c.font: "mono"
 each i in range 10
   a: a0+16*i
   t: "%04H %s" format a,mb4.showMemA[a]
   c.text[t (0,cs[1]*i+1)+ofs]
 end
end
 
on switch_tab name do 
 each t in extract value where value..name like "*_tab" from card.widgets
   t.show:"transparent"
 end
 me.show:"solid"
 each w in "lil_scripts","test_scripts","debugger"
   card.widgets[w].show:"none"
 end
 card.widgets[name].show:"solid"
end
{end}

