input:{"type":"field","size":[309,19],"pos":[1,187],"script":"itemlist.1","border":1,"scrollbar":0,"style":"code"}
status:{"type":"field","size":[309,37],"pos":[1,1],"locked":1,"script":"itemlist.1","show":"transparent","border":1,"scrollbar":0,"style":"code","value":"ds: []\ncs: []\nip: 1D2"}
cmd_s:{"type":"button","size":[22,20],"pos":[1,212],"font":"mono","text":"%s","style":"rect","shortcut":"s"}
cmd_e:{"type":"button","size":[22,20],"pos":[21,212],"font":"mono","text":"%e","style":"rect","shortcut":"g"}
cmd_r:{"type":"button","size":[22,20],"pos":[41,212],"font":"mono","text":"%R","style":"rect","shortcut":"r"}
button1:{"type":"button","size":[38,24],"pos":[272,208],"script":"b4console.1","font":"body","show":"transparent","text":"clear","style":"rect"}
cmd_C:{"type":"button","size":[22,20],"pos":[61,212],"font":"mono","text":"%C","style":"rect","shortcut":"r"}

{script:b4console.0}
mb4:deck.modules.mb4.value

mb4.set_echo[on echo line do
  t:output.text
  output.text: if t t,"\n",line else line end
  output.scroll: 999999
end]

on view do
  status.text
    : mb4.showStack["ds"], "\n"
    , mb4.showStack["cs"], "\n"
    , mb4.showIp[]
  show_ram[]
end

on run line do
 echo["> ",line]
 mb4.b4i[line]
 view[]
 show_ram[]
end

on click do
  run[me.text]
  deck.card.event["b4_step"]
end

# expose these methods
on get_run do run end
on get_clear do clear end

on clear do
  output.text:""
end

on show_ram do
 # start on debugger. draw some ram, highlight ip
 ip:mb4.get_vm[].ip
 a0: 256 gw:16 mw:31 cs: 18,18  ofs:3,4  
 on gxy i do (gw%i),floor i/gw end
 ixy: (ofs+(mw,0)-(3,3))+cs*gxy[ip-a0]+0,1
 c: card.widgets.debugger
 c.pattern: 47 c.rect[0,0 c.size]        # bg
 c.pattern: 35  c.rect[ixy cs+0,1]       # ip
 c.pattern: 41 c.rect[0,0 mw,c.size[1]]  # hex
 c.rect[0,0 c.size[0],cs[1]+ofs]
 c.pattern: 0 c.font: "mono"
 c.text[(list " +%H") format range 16 ofs+25,2]
 c.pattern: 0 c.font: "mono"
 each i in range 10
   a: a0+16*i
   t: "%04H %s" format a,mb4.showMemA[a]
   c.text[t (0,cs[1]*i+1)+ofs]
 end
end

{end}

{script:b4console.1}
on click do
  output.text:input.text:""
  show_ram[]
end
{end}

