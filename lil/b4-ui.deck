btn_s:{"type":"button","size":[19,18],"pos":[335,34],"font":"mono","text":"%s"}
button1:{"type":"button","size":[19,18],"pos":[414,34],"font":"mono","text":"%C"}

{script:b4_debugger.0}
mb4: deck.modules.mb4.value

on click do
  mb4.b4i[me.text]
  redraw[]
end

on redraw do
  labels.value:select label:key addr:value from mb4.get_labels[]
  # start on debugger. draw some ram, highlight ip
  ip_field.value: "%04H" format ip:mb4.get_ip[]
  a0: dbg_top.value gw:16 mw:31 cs: 18,18  ofs:3,4  
  on gxy i do (gw%i),floor i/gw end
  ixy: (ofs+(mw,0)-(3,3))+cs*gxy[ip-a0]+0,1
  c:mem_view
  c.pattern: 47 c.rect[0,0 c.size]        # bg
  c.pattern: 35  c.rect[ixy cs+0,1]       # ip
  c.pattern: 41 c.rect[0,0 mw,c.size[1]]  # hex
  c.rect[0,0 c.size[0],cs[1]+ofs]
  c.pattern: 0 c.font: "mono"
  c.text[(list " +%H") format range 16 ofs+25,2]
  c.pattern: 0 c.font: "mono"
  each i in range 10
    a: a0+16*i
    t: "%04H %s" format a,mb4.showMemA[a]
    c.text[t (0,cs[1]*i+1)+ofs]
  end
end

on get_redraw do redraw end 

{end}

{script:b4_debugger.1}
on change val do
  redraw[]
end
{end}

{script:b4_debugger.2}
on change val do
  mb4.set_ip["%h" parse val]
  redraw[]
end
{end}

{contraption:tabstrip}
size:[420,20]
resizable:1
margin:[0,2,0,3]
description:"A tab strip made of buttons.\n"
script:"tabstrip.0"
attributes:{"name":["tnames","wnames","bgcolor"],"label":["tab names","widget names","bg color"],"type":["string","string","number"]}
{widgets}
bgcolor:{"type":"slider","size":[48,16],"pos":[0,-128],"script":"tabstrip.1","interval":[0,48],"value":34,"style":"compact"}
wnames:{"type":"field","size":[384,16],"pos":[0,-64],"value":"w1|w2|w3"}
tnames:{"type":"field","size":[384,16],"pos":[0,-96],"value":"t1|t2|t3"}
which:{"type":"slider","size":[48,16],"pos":[64,-128],"script":"tabstrip.1","interval":[0,50],"style":"compact"}
button0:{"type":"button","size":[22,20],"pos":[0,0],"text":"t1","style":"rect"}
button1:{"type":"button","size":[22,20],"pos":[22,0],"show":"transparent","text":"t2","style":"rect"}
button2:{"type":"button","size":[22,20],"pos":[44,0],"show":"transparent","text":"t3","style":"rect"}
button3:{"type":"button","size":[60,20],"pos":[180,0],"show":"none","text":"d","style":"rect"}
button4:{"type":"button","size":[60,20],"pos":[240,0],"show":"none","text":"e","style":"rect"}
button5:{"type":"button","size":[60,20],"pos":[300,0],"show":"none","text":"f","style":"rect"}
button6:{"type":"button","size":[60,20],"pos":[360,0],"show":"none","text":"g","style":"rect"}
use_invert:{"type":"button","size":[140,20],"pos":[0,-160],"font":"body","show":"invert","text":"invert instead of color","style":"check","value":0}

{script:tabstrip.0}
on buttons do
  extract value
  where value..type="button" 
  where !value..name="use_invert"
  from card.widgets
end

on which_font do
  # card.font doesn't exist on the prototype
  if card.font card.font else deck.fonts.menu end
end

on view do draw[] end

on draw do
  # -- rebuild buttons
  p:0 nb:count bs: buttons[] bs..show:"none"
  each tn i in "|" split tnames.text
    if i<nb  b:bs[i]
      b.style:"rect"  b.pos:p,0   b.text:tn 
      p:p+w:(b.font:which_font[]).textsize[tn][0]+10
      b.size:w,card.size[1]
      b.show:if i=which.value "solid" elseif use_invert.value "invert" else "transparent" end
    end
  end
  # -- draw backgrounds
  c: bgcolor.value im:card.image.map[0 0] # clear bg
  each b in bs
    if !b.show="none"
      bg:im.copy[b.pos+1 b.size-2].map[0 c] im.paste[bg b.pos+1]
    end
  end
end

on click do
  ix:which.value:first extract index where value=me.text from "|" split tnames.text
  which.value:ix draw[] 
  dc:deck.card wns:"|"split wnames.text 
  (dc.widgets@wns)..show:"none"
  dc.widgets[wns[ix]].show:"solid"
  dc.event["set_tab" wn card.name]
end


on get_bgcolor do bgcolor.value end
on set_bgcolor x do bgcolor.value:x end
on get_tnames do tnames.text end
on get_wnames do wnames.text end
on set_wnames x do wnames.text:x end
on set_tnames x do tnames.text:x end
on get_which x do which.value end
on set_which x do which.value:x end

{end}

{script:tabstrip.1}
on change val do
  view[]
end
{end}

