{script:vgafont.2}
on chart_index xy do
 sum xy * 1,chart_gs[0]
end

on click pos do
 xy: floor pos/cs
 cursor.pos: me.pos + cs*xy
 cursor.size: cs
 ord.value: chart_index[xy]
 redraw_glyph[]
end

on drag pos do click[pos] end
{end}

